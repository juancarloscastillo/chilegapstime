@article{gros_bildungssysteme_2000,
  title = {Bildungssysteme, Soziale {{Ungleichheit}} Und Subjektive {{Schichteinstufung}}. {{Die}} Institutionelle {{Basis}} von {{Individualisierungsprozessen}} Im Internationalen {{Vergleich}}},
  author = {Gro{\ss}, Martin},
  year = {2000},
  month = oct,
  volume = {29},
