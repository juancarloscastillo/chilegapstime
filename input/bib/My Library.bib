@inproceedings{fairbrother_two_2013,
  title = {Two {{New Tests}} of the {{Relationship}} between {{Inequality}} and {{Political Discussion}}},
  booktitle = {{{ECPR}}},
  author = {Fairbrother, M.},
  year = {2013},
  address = {{Bourdeax}},
  abstract = {Does inequality discourage political engagement? Previous studies have noted that
people are less politically engaged in societies with higher income inequality\textemdash{}
possibly because all but the wealthiest conclude that politics is not about them and
their concerns. This cross-sectional association is not, however, proof of a causal
relationship; the association could be spurious, or due to reverse-causality, given that
other research suggests that disengagement contributes to higher inequality. Using
multilevel models fitted to data on the frequency of political discussion from all five
waves of the World/European Values Surveys, this paper re-assesses the thesis of a
causal relationship, in two ways. First, I investigate whether the relationship holds not
just cross-sectionally but also longitudinally. Second, I use an instrument for
inequality to test whether the correlation between inequality and discussion holds
even when ruling out reverse-causality. I find mixed evidence for a relationship over
time, but a clear correlation when instrumenting for inequality.}
}

