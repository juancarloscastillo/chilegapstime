@article{gerlitz_justice_2010,
  title = {Justice {{Perception}} in {{Times}} of {{Transition}}: {{Trends}} in {{Germany}}, 1991\textendash{}2006},
  shorttitle = {Justice {{Perception}} in {{Times}} of {{Transition}}},
  author = {Gerlitz, Jean-Yves and M{\"u}hleck, Kai and Scheller, Percy and Schrenker, Markus},
  year = {2010},
  month = dec,
  doi = {10.1093/esr/jcq062},
  abstract = {This article explores how the German distribution system is assessed with regard to justice in the fifteen years after unification. It traces the development of perceived justice for East and West Germany and tries to explain the regional differences and the trends observed. West Germans evaluate the system as more just throughout the entire period, though the gap shrinks. Surprisingly the main changes in the level of perceived justice did not take place in the eastern but in the western part. In 2006 West Germans assess the distribution system as considerably less just as right after unification. The level of consent is astonishingly volatile. In contrast, the assessment in East Germany is quite stable. How can this be explained? Three groups of factors are tested: socio-economic status, the overall economic performance, and justice ideologies. It is shown how the development of perceived justice is largely formed by levels of unemployment and changes in the affirmation of justice ideologies. Economic performance and justice beliefs are intertwined: as the economy fares badly its attitudinal basis weakens. We conclude that even in countries with a well established social market economy the approval of this distribution system depends to a large extent on its performance.},
  file = {/media/ntfs/Dropbox/zoterojcydocs/ya clasificados/Gerlitz et al. - 2010 - Justice Perception in Times of Transition Trends .pdf},
  journal = {European Sociological Review}
}

