@techreport{nunez_dime_2007,
  title = {``{{Dime}} C{\'o}mo Te Llamas y Te Dir{\'e} Qui{\'e}n Eres'': La {{Ascendencia}} Como Mecanismo de Diferenciaci{\'o}n Social En {{Chile}}},
